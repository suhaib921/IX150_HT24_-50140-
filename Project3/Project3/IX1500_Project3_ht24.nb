Cell["\<\
Definition of the function geoGraph which results in a list with the geo-data \
for the cities and links.\
\>", "Text",ExpressionUUID->"2cea0918-6378-433b-9e69-1f5a8d3a68c1"],

Cell[BoxData[
 RowBox[{
  RowBox[{"geoGraph", "[", 
   RowBox[{"v_", ",", "e_", ",", "c_", ",", "r_", ",", 
    RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"es", "=", 
       RowBox[{"OptionValue", "[", "EdgeStyle", "]"}]}], ",", 
      RowBox[{"vs", "=", 
       RowBox[{"OptionValue", "[", "VertexStyle", "]"}]}], ",", 
      RowBox[{"vl", "=", 
       RowBox[{"OptionValue", "[", "VertexLabels", "]"}]}], ",", 
      RowBox[{"vls", "=", 
       RowBox[{"OptionValue", "[", "VertexLabelStyle", "]"}]}], ",", 
      "\[IndentingNewLine]", "lbl", ",", 
      RowBox[{"\[CapitalDelta]", "=", 
       RowBox[{"{", 
        RowBox[{"0.1", ",", "0.3"}], "}"}]}]}], "\[IndentingNewLine]", "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "vl", "]"}], "\[NotEqual]", 
        RowBox[{"Length", "[", "v", "]"}]}], ",", 
       RowBox[{"vl", "=", "None"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"lbl", "=", 
      RowBox[{"If", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"vl", "===", "None"}], ",", 
        RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{"Join", "[", 
         RowBox[{
          RowBox[{"{", "vls", "}"}], ",", 
          RowBox[{"MapThread", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Text", "[", 
              RowBox[{"#1", ",", 
               RowBox[{"GeoPosition", "[", 
                RowBox[{"#2", "+", "\[CapitalDelta]"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"-", "1"}], ",", "0"}], "}"}]}], "]"}], "&"}], ",", 
            RowBox[{"{", 
             RowBox[{"vl", ",", "c"}], "}"}]}], "]"}]}], "]"}]}], 
       "\[IndentingNewLine]", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Join", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", "vs", "}"}], ",", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"GeoDisk", "[", 
             RowBox[{
              RowBox[{
              "c", "\[LeftDoubleBracket]", "#1", "\[RightDoubleBracket]"}], 
              ",", "r"}], "]"}], "&"}], ")"}], "/@", "v"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", "es", "}"}], ",", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"GeoPath", "[", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"c", "\[LeftDoubleBracket]", 
                RowBox[{
                "#1", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
                "\[RightDoubleBracket]"}], ",", 
               RowBox[{"c", "\[LeftDoubleBracket]", 
                RowBox[{
                "#1", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
                "\[RightDoubleBracket]"}]}], "}"}], "]"}], "&"}], ")"}], "/@",
           "e"}]}], "]"}], ",", "\[IndentingNewLine]", "lbl"}], 
      "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellLabel->
  "In[181]:=",ExpressionUUID->"ca09a04a-8856-42ae-ad74-9a45b257f682"],

