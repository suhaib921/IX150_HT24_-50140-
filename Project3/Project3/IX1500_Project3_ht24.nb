    RowBox[{
     RowBox[{"EdgeStyle", "\[Rule]", "Black"}], ",", 
     RowBox[{"VertexStyle", "\[Rule]", 
      RowBox[{"GeoStyling", "[", "None", "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"VertexLabels", "\[Rule]", "None"}], ",", 
