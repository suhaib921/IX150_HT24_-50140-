  FormBox[
   RowBox[{"V", "\[TildeTilde]", 
    RowBox[{"101", "\[Times]", 
     SuperscriptBox["10", "3"], 
     SuperscriptBox[
      StyleBox["m",
       FontSlant->"Plain"], "3"]}]}], TraditionalForm]],ExpressionUUID->
  "59c06eaa-9140-44fc-9b4c-16fe4f179e33"],
 "."
}], "Text",ExpressionUUID->"6dc335f4-7b1d-41d4-a2bd-24cc570aff2f"]
